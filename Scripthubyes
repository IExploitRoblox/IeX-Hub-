elseif game.PlaceId == 3652625463 then
        local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
        local Window = Library.CreateLib("Lifting Simulator - IeX Hub", "Midnight")

        --Main
        local Main = Window:NewTab("Main")
        local MainSection = Main:NewSection("Main")

	MainSection:NewToggle("Auto lifting", "Auto farm muscle", function(state)
        if state then
		getgenv().autoclick = true;
		while getgenv().autoclick == true do
		local ohTable1 = {
			[1] = "GainMuscle"
		}
		
		game:GetService("ReplicatedStorage").RemoteEvent:FireServer(ohTable1)
		   wait()
		end
        else
		getgenv().autoclick = false;
		while getgenv().autoclick == true do
			local ohTable1 = {
			[1] = "GainMuscle"
			}
		
			game:GetService("ReplicatedStorage").RemoteEvent:FireServer(ohTable1)
		   wait()
                        end            
                end
        end)

        MainSection:NewToggle("Auto sell", "Auto sell muscle into cash", function(state)
        if state then
                getgenv().autosell = true;
                while getgenv().autosell == true do
                        local ohTable1 = {
                                [1] = "SellMuscle"
                        }
                        
                        game:GetService("ReplicatedStorage").RemoteEvent:FireServer(ohTable1)
                wait()
                end
        else
                getgenv().autosell = false;
                while getgenv().autosell == true do
                        local ohTable1 = {
                                [1] = "SellMuscle"
                        }
                        
                        game:GetService("ReplicatedStorage").RemoteEvent:FireServer(ohTable1)
                wait()
                
                        end
                end
        end)

        --Teleport
        local Teleport = Window:NewTab("Teleport")
        local TeleportSection = Teleport:NewSection("Teleport")

        TeleportSection:NewButton("Goto Shop", "tp to shop", function()       
                humrp.CFrame = game:GetService("Workspace").EffectStorage.Mark02.CFrame
        end) 

