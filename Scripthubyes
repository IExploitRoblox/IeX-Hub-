                local A_1 = "Health"
                local A_2 = "All"
                local A_3 = 1000000000
                local A_4 = 0
                local Event = game:GetService("Players")["LocalPlayer"].RemoteFunctions["UR_ANTIHACK"]
                Event:FireServer(A_1, A_2, A_3, A_4)

        end)
        
        
        
            --Player
        local Player = Window:NewTab("Player")
        local PlayerSection = Player:NewSection("Player")

        PlayerSection:NewSlider("WalkSpeed", "Change walkspeed", 250, 16, function(s) -- 500 (MaxValue) | 0 (MinValue)
                game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
            end)

        PlayerSection:NewSlider("JumpPower", "Change jumppower", 250, 50, function(s) -- 500 (MaxValue) | 0 (MinValue)
                game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
            end) 

        --Settings
        local Settings = Window:NewTab("Settings")
        local SettingsSection = Settings:NewSection("Settings")

        SettingsSection:NewKeybind("ToggleUI", "ToggleUI keybind", Enum.KeyCode.RightControl, function()
                Library:ToggleUI()
        end)

elseif game.PlaceId == 2986677229 then
        local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
        local Window = Library.CreateLib("The Legendary Sword RPG - IeX Hub", "Midnight")
            
        --Main
        local Main = Window:NewTab("Main")
        local MainSection = Main:NewSection("Main")
            
        MainSection:NewToggle("Auto exp", "Auto farm exp", function(state)
                if state then
                        getgenv().expfarm = true;
                        while getgenv().expfarm == true do
                        local Event = game:GetService("ReplicatedStorage").Aero.AeroRemoteServices.GameService.WeaponAttackStart
                        Event:FireServer()
                    wait()
                        local Event = game:GetService("ReplicatedStorage").Aero.AeroRemoteServices.GameService.WeaponAnimComplete
                        Event:FireServer()
                        end
                else
                        getgenv().expfarm = false;
                        while getgenv().expfarm == true do
                        local Event = game:GetService("ReplicatedStorage").Aero.AeroRemoteServices.GameService.WeaponAttackStart
                        Event:FireServer()
                    wait()
                        local Event = game:GetService("ReplicatedStorage").Aero.AeroRemoteServices.GameService.WeaponAnimComplete
                        Event:FireServer()   
                        end            
                end
        end)

        MainSection:NewToggle("Auto open lvl crate", "Auto open crate that got upon lvl up", function(state)
                if state then
                getgenv().autocrate = true;
                        while getgenv().autocrate == true do
                        local A_1 = "t1_wpn_crate"
                        local Event = game:GetService("ReplicatedStorage").Aero.AeroRemoteServices.GameService.OpenCrate
                        Event:InvokeServer(A_1)
                                wait()
                        local A_1 = "t1_skin_crate"
                        local Event = game:GetService("ReplicatedStorage").Aero.AeroRemoteServices.GameService.OpenCrate
                        Event:InvokeServer(A_1)
                        wait()
                        end

                else
                getgenv().autocrate = false;
                        while getgenv().autocrate == true do
                        local A_1 = "t1_wpn_crate"
                        local Event = game:GetService("ReplicatedStorage").Aero.AeroRemoteServices.GameService.OpenCrate
                        Event:InvokeServer(A_1)
                                wait()
                        local A_1 = "t1_skin_crate"
                        local Event = game:GetService("ReplicatedStorage").Aero.AeroRemoteServices.GameService.OpenCrate
                        Event:InvokeServer(A_1)
                        wait()
                        end
                end
        end)

