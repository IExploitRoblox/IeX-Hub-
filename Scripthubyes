local char = game.Players.LocalPlayer.Character
local hum = char:WaitForChild("Humanoid")
local humrp = char.HumanoidRootPart


if game.PlaceId == 60654525 then
        local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
        local Window = Library.CreateLib("The Legendary Sword RPG - IeX Hub", "Midnight")
    
        --Main
        local Main = Window:NewTab("Main")
        local MainSection = Main:NewSection("Main")
    
        MainSection:NewButton("Get best sword", "Get the best sword in the game", function()       
            game.Players.LocalPlayer.RemoteFunctions.SwordShopSystem:FireServer("Buy", 0,"The Devil's Blade")
        end)
    
        MainSection:NewButton("Infinite money", "Get unlimited money", function()        
            game.Players.LocalPlayer.RemoteFunctions.SwordShopSystem:FireServer("Sell", 1000000000,"Bronze")
        end)
    
        --Stats
        local Stats = Window:NewTab("Stats")
        local StatsSection = Stats:NewSection("Stats")
    
        StatsSection:NewButton("Auto Health", "Auto use point on health", function()
                local A_1 = "Health"
                local Event = game:GetService("Players").LocalPlayer.RemoteFunctions.UpgradeFunction
                Event:FireServer(A_1)
        end)
        StatsSection:NewButton("Auto damage", "Auto use point on damage", function()
                local A_1 = "Damage"
                local Event = game:GetService("Players").LocalPlayer.RemoteFunctions.UpgradeFunction
                Event:FireServer(A_1)
        end)
        StatsSection:NewButton("Auto defense (go over limit)", "Bypass limit make you get hp instand of damage", function()
    
                local A_1 = "Defense"
                local Event = game:GetService("Players").LocalPlayer.RemoteFunctions.UpgradeFunction
                Event:FireServer(A_1)
        end)
        StatsSection:NewButton("Auto gold gained", "Pretty useless cant you got like 500m", function()
                local A_1 = "Loot"
                local Event = game:GetService("Players").LocalPlayer.RemoteFunctions.UpgradeFunction
                Event:FireServer(A_1)
        end)
    
        --Player
        local Player = Window:NewTab("Player")
        local PlayerSection = Player:NewSection("Player")
    
        PlayerSection:NewSlider("Walkspeed", "Changes the walkspeed", 250, 16, function(v)
                game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = v
        end)
        PlayerSection:NewSlider("Jumppower", "Changes the jumppower", 250, 50, function(v)
                game.Players.LocalPlayer.Character.Humanoid.JumpPower = v
        end)
        
        --Others
    
        local Others = Window:NewTab("Others")
        local OthersSection = Others:NewSection("Others")
    
        OthersSection:NewButton("Max Rebirth", "Get Max Rebirth", function()
                game.Players.LocalPlayer.RemoteFunctions.RebirthFunction:FireServer("25")
        end)
        OthersSection:NewButton("100 Rebirth", "Get 100 rebirth now people sus of you", function()
                game.Players.LocalPlayer.RemoteFunctions.RebirthFunction:FireServer("100")
        end)
    end
